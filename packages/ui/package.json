{
  "name": "@snappmarket/ui",
  "private": false,
  "version": "0.2.1-alpha.2",
  "author": "Jafar Rezaei (SayJeyHi) <jafar.rezaei.ard@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "yarn build:cjs && yarn build:esm && yarn build:es && yarn build:umd && yarn build:copy-files",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ../../babel.config.js ./packages --out-dir ./dist",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ../../babel.config.js ./packages --out-dir ./dist/esm",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ../../babel.config.js ./packages --out-dir ./dist/es",
    "build:umd": "cross-env BABEL_ENV=production-umd rollup -c",
    "build:copy-files": "node ../../scripts/copy-files.js",
    "prebuild": "rimraf dist",
    "release": "yarn build && npm publish dist --tag latest",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "@snappmarket/helpers": "^0.2.1-alpha.2",
    "@snappmarket/hooks": "^0.2.1-alpha.0",
    "@snappmarket/icons": "^0.2.1-alpha.0",
    "polished": "^3.4.2",
    "react-hgs-input-range": "^1.3.1",
    "react-image": "^2.2.2",
    "react-router-dom": "^5.1.2",
    "react-slick": "^0.25.2"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "license": "MIT",
  "keywords": [
    "ui-components",
    "components",
    "accordion",
    "alert",
    "breadcrumb",
    "button",
    "checkbox",
    "confirm-dialog",
    "dialog",
    "grid",
    "grid-system",
    "icon",
    "image",
    "input",
    "input-range",
    "range",
    "range-slider",
    "json-ld",
    "label",
    "lined-text",
    "loading",
    "modal",
    "option-group",
    "radio",
    "pagination",
    "progress-bar",
    "skeleton",
    "slider",
    "tab",
    "tab-switcher",
    "textarea",
    "text-wrap",
    "ellipsis-text",
    "toggle",
    "tooltip"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/snappmarket/frontend-toolbox"
  },
  "sideEffects": false,
  "homepage": "https://github.com/snappmarket/frontend-toolbox/tree/master/packages/ui",
  "gitHead": "a5f063027da3f3200d940137c39f875842280b46",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3"
  }
}
