{
  "name": "@snappmarket/hooks",
  "private": false,
  "version": "0.1.28",
  "author": "Jafar Rezaei (SayJeyHi) <jafar.rezaei.ard@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "yarn build:cjs && yarn build:esm && yarn build:es && yarn build:umd",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ../../babel.config.js ./packages --out-dir ./dist --ignore \"**/*.spec.js\"",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ../../babel.config.js ./packages --out-dir ./dist/esm --ignore \"**/*.spec.js\"",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ../../babel.config.js ./packages --out-dir ./dist/es --ignore \"**/*.spec.js\"",
    "build:umd": "cross-env BABEL_ENV=production-umd rollup -c",
    "prebuild": "rimraf dist",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "react-router-dom": "^5.1.2"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "license": "MIT",
  "keywords": [
    "hook",
    "react-hooks",
    "custom-hook",
    "debounce",
    "useDebounce",
    "debounce-hook",
    "useEffect-update",
    "useUpdateEffect",
    "useDidUpdateEffect",
    "useFocus",
    "focus-hook",
    "useForceUpdate",
    "forceUpdate-hook",
    "geolocation-hook",
    "useGeolocation",
    "routeChangeDetect",
    "routeChange-hook",
    "useRouteChange",
    "useTimer",
    "timer-hook"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/snappmarket/frontend-toolbox"
  },
  "gitHead": "2207934c0e1e69a5004aee326f2ccfabe910ee32"
}
